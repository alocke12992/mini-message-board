<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to the AWESOME <%= title %></p>
    <ul>
      <% messages.forEach(function(message) { %>
      <li>
        <div>
          <%= message.user %> 
          <%= message.text %> 
          <%= message.added %>
        </div>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
